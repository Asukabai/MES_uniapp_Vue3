        "navigationBarBackgroundColor": "#ffffff",
        "navigationBarTextStyle": "white",
        "h5": {
          "title": "首页"
        }
      }
    },
    {
      "path": "pages/activity/index",
      "aliasPath": "/activity",
      "style": {
        "navigationBarTitleText": "活动推荐",
